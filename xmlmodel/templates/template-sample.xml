<Template>
    <I type = "add reg32, imm32" inip = "1.0"> </I>
    <traverse var = "mem" type = "mem16">
        <I type = "lea bx, @mem"></I>
        <I type = "add @mem, imm16"></I>
        <sequence>
            <I type = "sub @mem, imm16"></I>
        </sequence>
    </traverse>
    <repeat times = "10">
        <select>
            <G type = "BinaryArithmetic" weight = "35"> </G>
            <G type = "Logical" weight = "25"> </G>
            <G type = "ShiftRotate" weight = "15"> </G>
            <G type = "DataTransfer" weight = "25"> </G>
        </select>
    </repeat>
    <C type = "x86_state"> </C>
    <C type = "al"> </C>
    <C type = "ah"> </C>
    <C type = "ax"> </C>
    <C type = "eax"> </C>
    <C type = "cs"> </C>
    <C type = "eflags"> </C>
    <traverse var = "reg" type = "reg16">
        <I type = "add @reg,imm16"></I>
        <C type = "@reg"></C>
    </traverse>
</Template>
