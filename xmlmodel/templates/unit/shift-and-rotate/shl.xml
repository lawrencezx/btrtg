<?xml version = "1.0" ?>

<!--
@Test: SHL
@Description: test shl single instruction
@Detail:
@Author: ZouXu
-->

<Template>
    <traverse>
        <V var = "reg" type = "reg8"> </V>
        <I type = "shl @reg, cl" trv="true"></I>
        <C type = "@reg"></C>
    </traverse>
    <traverse>
        <V var = "mem" type = "mem8"> </V>
        <I type = "shl @mem, imm8" trv="true"></I>
        <I type = "mov al, @mem"></I>
        <C type = "al"></C>
    </traverse>

    <traverse>
        <V var = "reg" type = "reg16"> </V>
        <I type = "shl @reg, cl" trv="true"></I>
        <C type = "@reg"></C>
    </traverse>
    <traverse>
        <V var = "mem" type = "mem16"> </V>
        <I type = "shl @mem, imm8" trv="true"></I>
        <I type = "mov ax, @mem"></I>
        <C type = "ax"></C>
    </traverse>

    <traverse>
        <V var = "reg" type = "reg32"> </V>
        <I type = "shl @reg, cl" trv="true"></I>
        <C type = "@reg"></C>
    </traverse>
    <traverse>
        <V var = "mem" type = "mem32"> </V>
        <I type = "shl @mem, imm8" trv="true"></I>
        <I type = "mov eax, @mem"></I>
        <C type = "eax"></C>
    </traverse>
</Template>
