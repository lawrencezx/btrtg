<Template>
    <traverse>
        <V var = "reg" type = "reg16"> </V>
        <I type = "shrd @reg,reg16,imm8" trv="true"></I>
        <C type = "@reg"></C>
    </traverse>
    <traverse>
        <V var = "mem" type = "mem16"> </V>
        <I type = "shrd @mem,reg16,imm8" trv="true"></I>
        <I type = "mov ax, @mem"></I>
        <C type = "ax"></C>
    </traverse>

    <traverse>
        <V var = "reg" type = "reg16"> </V>
        <I type = "shrd @reg,reg16,cl" trv="true"></I>
        <C type = "@reg"></C>
    </traverse>
    <traverse>
        <V var = "mem" type = "mem16"> </V>
        <I type = "shrd @mem,reg16,cl" trv="true"></I>
        <I type = "mov ax, @mem"></I>
        <C type = "ax"></C>
    </traverse>

    <traverse>
        <V var = "reg" type = "reg32"> </V>
        <I type = "shrd @reg,reg32,imm8" trv="true"></I>
        <C type = "@reg"></C>
    </traverse>
    <traverse>
        <V var = "mem" type = "mem32"> </V>
        <I type = "shrd @mem,reg32,imm8" trv="true"></I>
        <I type = "mov eax, @mem"></I>
        <C type = "eax"></C>
    </traverse>

    <traverse>
        <V var = "reg" type = "reg32"> </V>
        <I type = "shrd @reg,reg32,cl" trv="true"></I>
        <C type = "@reg"></C>
    </traverse>
    <traverse>
        <V var = "mem" type = "mem32"> </V>
        <I type = "shrd @mem,reg32,cl" trv="true"></I>
        <I type = "mov eax, @mem"></I>
        <C type = "eax"></C>
    </traverse>
</Template>
